        </mj-column>
        </mj-group>
       
            <mj-group  width="200px">
             <mj-column   width="200px" >
            <mj-image css-class="app" width="100px" mj-class="apple" href="https://www.momondo.com/" src="./assets/appstore.png" alt="" />

        </mj-column>
        </mj-group>
        
            
