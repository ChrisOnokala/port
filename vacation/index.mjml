                <mj-button css-class="hover-btn" href="https://www.momondo.com/" mj-class="img-btn">
                    <mj-text  mj-class="img-btn-link">
                        See Deal
                    </mj-text>
                </mj-button>
            </mj-column>
        </mj-section>

        <mj-raw>
            <!-- Next Section -->
        </mj-raw>
        <mj-section mj-class="img-section-last" background-color="#dddddd"  >
            <mj-column mj-class="img-col"  background-color="#ffffff"  >
                <mj-image mj-class="img-text-image" height="300px" src="./assets/other-destinations.jpg" alt="" />
